.zhezhao {
    position: fixed;
    width: 100%;
    height: auto;
    top: 0px;
    overflow: hidden;
  }
   
  @keyframes myfirst {
    from {
      transform: translateY(-860rpx);
    }
   
    to {
      transform: translateY(0rpx);
    }
  }
   
  @keyframes mylast {
    from {
      transform: translateY(0rpx);
    }
   
    to {
      transform: translateY(-900rpx);
    }
  }
   
  .test2 {
    animation: mylast;
    animation-duration: 1s;
    animation-iteration-count: 1;
    animation-fill-mode: forwards;
    width: 750rpx;
    height: auto;
    border: 2rpx solid #ededed;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1454;
    background-color: #fff;
  }
   
  .test1 {
    animation: myfirst;
    animation-duration: 0.5s;
    animation-iteration-count: 1;
    animation-fill-mode: forwards;
    width: 750rpx;
    height: auto;
    border: 2rpx solid #ededed;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1454;
    background-color: #fff;
  }
   
  .btn {
    width: 700rpx;
    height: auto;
    position: absolute;
    bottom: 0;
    left: 0;
    margin: 10rpx;
    margin-left: 25rpx;
    
  }
   
  .test3{
    display: flex;
    flex-direction: column;
    background-color: #ededed;
   
  }
   
  .input-content{
    width: 100%;
    height: auto;
    border:2rpx solid #ededed;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 40rpx;
    background-color: #fff;
  }
   
  .ta{
    width: 650rpx;
    height: auto;
    margin-bottom: 20rpx;
    margin-top: 110rpx;
    /* border: 8rpx solid red;  */
    margin-left: auto;
    margin-right: auto;
  }