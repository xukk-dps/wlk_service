 .J-shopping-cart-empty{    
     margin: 0;    
     padding: 0;    
     border: 0;    
     font: inherit;    
     font-size: 100%;    
     vertical-align: baseline;
}
.shopping-cart-empty {   
    height: 250px;    
    padding-bottom: 1.6rem;     
    text-align: center;    
    position: relative;
} 
.shopping-cart-empty .bg{    
    position: absolute;    
    width: 16.29333rem;    
    height: 6.73333rem;    
    left: 50%;    
    -webkit-transform: translateX(-50%);    
    transform: translateX(-50%);      
    background-size: 100%;    
    top: 0;
} 
.shopping-cart-empty .cart{        
    width: 5.83333rem;    
    height: 7.46rem;    
    background-size: 100%;    
    margin: 0 auto;
}  
.shopping-cart-empty .button {   
